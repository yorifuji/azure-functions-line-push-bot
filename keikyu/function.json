{
  "disabled": false,
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */5 7-9,16-20 * * 1-5"
    },
    {
      "type": "queue",
      "name": "outputQueueItem",
      "queueName": "outgoing-queue",
      "connection": "AzureWebJobsDashboard",
      "direction": "out"
    },
    {
      "type": "queue",
      "name": "outputSlackQueue",
      "queueName": "outgoing-slack-queue",
      "connection": "AzureWebJobsDashboard",
      "direction": "out"
    },
    {
      "type": "documentDB",
      "name": "inputDocument",
      "databaseName": "twitter",
      "collectionName": "MyCollection",
      "connection": "yorifuji-line-bot-db2_DOCUMENTDB",
      "direction": "in"
    },
    {
      "type": "documentDB",
      "name": "outputDocument",
      "databaseName": "twitter",
      "collectionName": "MyCollection",
      "createIfNotExists": true,
      "connection": "yorifuji-line-bot-db2_DOCUMENTDB",
      "direction": "out"
    }
  ]
}

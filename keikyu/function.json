{
  "disabled": false,
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */5 7-9,16-20 * * 1-5"
    },
    {
      "type": "queue",
      "name": "outputQueueItem",
      "queueName": "outgoing-queue",
      "connection": "AzureWebJobsDashboard",
      "direction": "out"
    },
    {
      "type": "queue",
      "name": "outputSlackQueue",
      "queueName": "outgoing-slack-queue",
      "connection": "AzureWebJobsDashboard",
      "direction": "out"
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "twitter/data",
      "connection": "AzureWebJobsDashboard",
      "direction": "out"
    },
    {
      "type": "blob",
      "name": "inputBlob",
      "path": "twitter/data",
      "connection": "AzureWebJobsDashboard",
      "direction": "in"
    }
  ]
}
